<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="403.0" prefWidth="567.0" GridPane.columnIndex="1">
         <children>
            <Button layoutX="138.0" layoutY="151.0" mnemonicParsing="false" onAction="#parseBlue" text="Blue" textFill="#196bcf">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font></Button>
            <Button layoutX="24.0" layoutY="47.0" mnemonicParsing="false" onAction="#parseR" text="Red" textFill="#d31414">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font></Button>
            <Button layoutX="35.0" layoutY="73.0" mnemonicParsing="false" onAction="#parseO" text="Orange" textFill="#e87800">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font></Button>
            <Button layoutX="57.0" layoutY="99.0" mnemonicParsing="false" onAction="#parseY" prefHeight="26.0" prefWidth="72.0" text="Yellow" textFill="#f8ff00">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font></Button>
            <Button layoutX="107.0" layoutY="125.0" mnemonicParsing="false" onAction="#parseG" text="Green" textFill="#00f765">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
