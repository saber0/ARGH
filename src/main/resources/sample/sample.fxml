            <Button layoutX="179.0" layoutY="202.0" mnemonicParsing="false" onAction="#parseV" text="Violet" textFill="#dd1616">
               <font>
                  <Font name="System Bold Italic" size="13.0" />
               </font></Button>
            <MenuBar>
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#fileRead" text="Open" />
                        <MenuItem mnemonicParsing="false" onAction="#clear" text="Clear" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#helpMsg" text="Instructions" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
            <Button layoutX="17.0" layoutY="235.0" mnemonicParsing="false" onAction="#otherWay" text="There Must Be Another Way" />
            <Button layoutX="126.0" layoutY="66.0" mnemonicParsing="false" onAction="#yetAnotherWay" text="Button" />
